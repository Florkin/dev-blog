
{% extends 'admin/layout.twig' %}

{% block head %}
    <title>Ajouter un post</title>
    <script src="https://cdn.tiny.cloud/1/no-api-key/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>
{% endblock %}

{% block content %}



    <form class="form" method="post" action="{{ actionAddpost }}" enctype="multipart/form-data">
        {% for formfield in postForm %}
            <div class="form-group">
                <div>{{ formfield|raw }}</div>
            </div>
        {% endfor %}
    </form>
</div>

{% endblock %}